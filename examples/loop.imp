[ loop.imp - zagniezdzone petle 

        Przykladowe dane testowe:
        1. ? 3
           > 30000003
           > 3000001
]
DECLARE
	a, b, c(1234:1235)
BEGIN
	READ a;
	b := 10;
	c(1234) := 1;
	c(1235) := c(1234) * 134217727; [ c(1235) = 2^27 - 1 ]
	c(1235) := c(1235) / 134217727; [ c(1235) = 1 ]
	FOR k FROM 999999 DOWNTO 0 DO
		FOR i FROM 1 TO 3 DO
			a := a + b;
			c(1234) := c(1234) + c(1235);
		ENDFOR
	ENDFOR
	WRITE a;
	WRITE c(1234);
END


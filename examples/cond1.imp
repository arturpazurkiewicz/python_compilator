[ cond1.imp - instrukcje warunkowe 

        Przykladowe dane testowe:
        1. ? 2030
           > 2035
           > 0
]
DECLARE
	a, b
BEGIN
	READ a;
	a := 5 + a;
	b := a - 2047;
	b := b / 1;
    a := a * 1;
	IF a < b THEN
		WRITE 111111;
	ELSE
		WRITE a;
	ENDIF
	IF b <= a THEN
		WRITE b;
	ELSE
		WRITE 111111;
	ENDIF
END

